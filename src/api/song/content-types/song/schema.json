{
  "kind": "collectionType",
  "collectionName": "songs",
  "info": {
    "singularName": "song",
    "pluralName": "songs",
    "displayName": "song"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "media": {
      "allowedTypes": [
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "plays": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "artists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist"
    },
    "player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::player.player",
      "mappedBy": "song"
    }
  }
}
